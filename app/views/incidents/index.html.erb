<h1>Incidents#index</h1>
<p>Find me in app/views/incidents/index.html.erb</p>
<p><%= link_to "New incident", incidents_new_path %></p>
<table>
  <thead>
    <td>Priority</td>
    <td>ID</td>
    <td>Title</td>
    <td>Source</td>
    <td></td>
    <td></td>
  </thead>
  <tbody>
  <% @incidents.each do |i| %>
    <tr>
      <td><%= i.priority %></td>
      <td><%= i.id %></td>
      <td><%= i.title %></td>
      <td><%= i.source %></td>
      <td><%= link_to "show", incident_path(i) %></td>
      <td><%= link_to "edit", edit_incident_path(i) %></td>
    </tr>
  <% end %>
  </tbody>
</table>